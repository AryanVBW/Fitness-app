<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">


    <uses-permission android:name="android.permission.ACTIVITY_RECOGNITION" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/name"
        android:theme="@style/Theme.FitnessTracker">
        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DietActivity" android:exported="false" />
        <activity android:name=".ReportActivity" android:exported="false" />
        <activity android:name=".ChestWorkoutDetailActivity" android:exported="false" />
        <activity android:name=".PushupsDetailActivity" android:exported="false" />
        <activity android:name=".CardioDetailActivity" android:exported="false" />
        <activity android:name=".ChestWorkoutActivity" android:exported="false" />
    </application>

</manifest>
